<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/remixicon@2.5.0/fonts/remixicon.css" rel="stylesheet">
    <link rel="stylesheet" href="./stylesheets/myproduct.css">
</head>
<body>
    <div class="products">
     
     
        <% loggedinuser.product.forEach(function(elem){ %>
          <div class="product">
          
            <div class="top">
              <img src="<%= elem.img%>" alt="">
            </div>
           
            <div class="btm">
              <h3>
                <%=elem.name%>
              </h3>
              <h4>
                <%=elem.price%>
              </h4>
              <div class="icons">
                <div class="circle"><a href=""><i class="ri-edit-box-line"></i></a></div>
                <button class="bid"><a href="/viewBid/<%= elem._id%>"> View bids</a></button>
                <div class="circle"><a href="/delete/<%= elem._id %>"><i class="ri-delete-bin-6-line"></i></a></div>
              </div>
            </div>
          </div>
          <% }) %>

    </div>
</body>
</html>